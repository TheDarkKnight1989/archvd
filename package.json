{
  "name": "archvd",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --webpack",
    "build": "next build --webpack",
    "start": "next start",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "test": "playwright test",
    "test:smoke": "playwright test tests/smoke",
    "test:ui": "playwright test --ui",
    "seed:market": "npx dotenv -e .env.local -- npx tsx scripts/seed_market.ts",
    "seed:fx": "set -a && source .env.local && set +a && node scripts/populate-fx-rates.mjs",
    "seed:pokemon": "npx dotenv -e .env.local -- npx tsx scripts/seed_pokemon.ts",
    "seed:sneakers": "npx dotenv -e .env.local -- npx tsx scripts/seed_sneaker_mock.ts",
    "seed:portfolio": "npx dotenv -e .env.local -- npx tsx scripts/seed_portfolio_demo.ts",
    "seed:all": "npx dotenv -e .env.local -- node scripts/seed-comprehensive.mjs",
    "seed:market:bridge": "set -a && source .env.local && set +a && node scripts/seed_market_bridge.mjs && npm run refresh:mvs",
    "migrate:fixed": "set -a && source .env.local && set +a && ./scripts/apply-fixed-migrations.sh",
    "migrate:market": "npx dotenv -e .env.local -- node scripts/apply-market-migration.mjs",
    "check:schema": "node scripts/check-schema.mjs",
    "sync:alias": "node scripts/sync-alias.mjs",
    "sync:stockx": "npx dotenv -e .env.local -- node scripts/sync-stockx.mjs",
    "sync:market:catalog": "npx dotenv -e .env.local -- node scripts/sync-market-catalog.mjs",
    "sync:market:prices": "npx dotenv -e .env.local -- node scripts/sync-market-prices.mjs",
    "sync:market:sales": "npx dotenv -e .env.local -- node scripts/sync-market-sales.mjs",
    "backfill:market:prices": "npx dotenv -e .env.local -- node scripts/backfill-market-prices.mjs",
    "refresh:mvs": "npx dotenv -e .env.local -- node scripts/refresh-mvs.mjs",
    "map:shopify-alias": "node scripts/map-shopify-to-alias.mjs",
    "prepare": "husky"
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.79.0",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.12",
    "@types/papaparse": "^5.5.0",
    "@types/uuid": "^10.0.0",
    "cheerio": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.552.0",
    "next": "16.0.1",
    "next-themes": "^0.4.6",
    "papaparse": "^5.5.3",
    "pg": "^8.16.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "recharts": "^3.3.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "uuid": "^13.0.0",
    "xlsx": "^0.18.5",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4",
    "@types/cheerio": "^0.22.35",
    "@types/node": "^20.19.24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "husky": "^9.1.7",
    "tailwindcss": "^4",
    "tsx": "^4.19.2",
    "typescript": "^5"
  }
}
